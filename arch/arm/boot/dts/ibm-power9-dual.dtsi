/* Instantiate chip 0 */
#define CFAM_CHIP_ID 0
&fsi {
	cfam@0,0 {
		reg = <0 0>;
		#include "ibm-power9-cfam.dtsi"
	};
};
#undef CFAM_CHIP_ID

/* Instantiate chip 1 */
#define CFAM_CHIP_ID 1
&fsi_hub0 {
	cfam@1,0 {
		reg = <1 0>;
		#include "ibm-power9-cfam.dtsi"
	};
};
#undef CFAM_CHIP_ID

/* Legacy OCC numbering (to get rid of when userspace is fixed) */
&fsi_occ0 {
	reg = <1>;
};

&fsi_occ1 {
	reg = <2>;
};

